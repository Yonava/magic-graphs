{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./dist/types",
    "rootDir": "./src",
    "paths": {
      "@magic/server/*": ["./src/*"],
      "@magic/graph/*": ["../graph/src/*"]
    },
    "moduleResolution": "nodenext",
    "module": "nodenext",
    "emitDeclarationOnly": false
  },
  "references": [{ "path": "../graph" }],
  "include": ["./src/**/*"],
  "exclude": ["./dist", "**/node_modules"]
}
