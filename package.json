{
  "name": "magic-graphs",
  "version": "1.0.0",
  "scripts": {
    "start": "cd packages/server && pnpm i --omit=dev && node dist/index.js",
    "dev": "concurrently -c auto command 'pnpm:dev-*'",
    "dev-client": "cd packages/client && pnpm dev",
    "dev-server": "cd packages/server && pnpm dev",
    "reinstall": "pnpm store prune && rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "build:types": "vue-tsc -b tsconfig.json"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "concurrently": "^8.2.2",
    "typescript": "~5.4.0",
    "vite": "^5.4.19",
    "vue-tsc": "^3.0.7"
  },
  "dependencies": {
    "primevue": "^4.2.4",
    "ts-essentials": "^10.1.1",
    "vue": "^3.5.18"
  }
}
